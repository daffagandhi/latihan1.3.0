package Mahasiswa;

/**
 *
 * @author Daffa Gandhi
 */
public class Mahasiswa {
    String membaca;
    int nyontek;
    String modifikasi;
    
    public void Mahasiswa(){
        System.out.println("cara membaca" + membaca + "dengan mimik wajah");
                           
        System.out.println("sekitar" + nyontek + "cara mencontek saat ujian");
        System.out.println("cara modifikasi contekan " + modifikasi);
    }
}

package Mahasiswa;

/**
 *
 * @author Daffa Gandhi
 */
public class Mahasiswaberaksi {
    public static void main(String[] args) {
        Mahasiswa mahasiswauin = new Mahasiswa();
        
                  mahasiswauin.membaca = "puisi";
                  mahasiswauin.nyontek = membawa kertas contekan;
                  mahasiswauin.modifikasi = "ada beberapa cara semisal mengacak urutan";
                  mahasiswauin.Mahasiswa();
                  
                  
                  
        }
    }
